# -*- mode: conf; -*-
gap_y -1000
 
background no
double_buffer yes 
out_to_console yes 
own_window yes 
own_window_type desktop
update_interval 1.0 
#font -*-terminus-*-*-*-*-12-*-*-*-*-*-iso8859-*
 
TEXT
^i(.dzen/sm4tik/cpu.xbm) ${cpu cpu0}% ${cpu cpu1}% ${cpu cpu2}% ${cpu cpu3}% \
^i(.dzen/sm4tik/mem.xbm) $memperc% \
^i(.dzen/sm4tik/arch.xbm) ${entropy_avail}/${entropy_poolsize} \
^i(.dzen/sm4tik/clock.xbm) ${time %b %e %H:%M:%S}