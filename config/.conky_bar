# -*- mode: conf; -*-
gap_y -1000
 
background no
double_buffer yes 
out_to_console yes 
own_window yes 
own_window_type desktop
update_interval 1.0 
#font -*-terminus-*-*-*-*-12-*-*-*-*-*-iso8859-*


TEXT
^i(/home/johannes/.dzen/rob/cpu.xbm) \
^fg(\#FFFFFF)${cpu}% \
^i(/home/johannes/.dzen/rob/mem.xbm) \
^fg(\#FFFFFF)${memperc}% \
^fg(\#ebac54) ${if_existing /proc/net/route eth1} \
^i(/home/johannes/.dzen/rob/net_wired.xbm) \
^fg(\#00aa4a) \
^i(/home/johannes/.dzen/rob/net_down_03.xbm)${downspeed eth1} \
^fg(\#ff3333) \
^i(/home/johannes/.dzen/rob/net_up_03.xbm)${upspeed eth1} \
^fg(\#ebac54) \
^i(/home/johannes/.dzen/rob/mpd.xbm) \
^fg(\#00ffff)${exec rhythmbox-client --print-playing-format "%aa-%at-%tt" | cut -c-70} \
^fg(\#ebac54) \
^i(/home/johannes/.dzen/rob/volume.xbm) \
^fg(\#00ffff)${exec amixer get Headphone | egrep -o "[0-9]+%" | head -1 | egrep -o "[0-9]*"}% \
^fg(\#ebac54) \
^i(/home/johannes/.dzen/rob/clock.xbm) \
^fg(\#FFFFFF) ${time %d %m %Y} \
^fg(\#ebac54) ${time %R}